<script id="toolbar_pages_show_tmpl" type="text/x-jquery-tmpl">
	<dl>
		<dt>主导航<br />区域</dt>
		<dd>
			{{if Pages.length > 0}}
			<div style="float: left"><a href="javascript:void(0)" is_home="true" class="page_link" page_id="${Pages[0].PageId}" index_id="${Pages[0].IndexId}">${Pages[0].Name}</a></div>
			<ul class="toolbar_page">
				{{each(i, page) Pages}}
					{{if i == 0}}
					{{else}}
						<li><a href="javascript:void(0)" class="page_link" page_id="${page.PageId}" index_id="${page.IndexId}">${page.Name}</a></li>
					{{/if}}
				{{/each}}
			</ul>
			{{/if}}
			<div class="new_page header_button_f" style="display:none"></div>
		</dd>
	</dl>
	<p class="toolbar_edit_border">注：首页主导航上面的分类文字链接的页面</p>
</script>

