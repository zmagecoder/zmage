<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" 
	"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">  
<generatorConfiguration>
	
	<properties resource="config/jdbc.properties"></properties>
	
    <classPathEntry  location="${driver.path}"/>  
    <context id="DB2Tables"  targetRuntime="MyBatis3">
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="true"/>  
        </commentGenerator>
         
        <jdbcConnection driverClass="${mysql.driverClass}" 
        	connectionURL="${mysql.jdbcUrl}" 
        	userId="${mysql.username}" password="${mysql.password}">
        </jdbcConnection>
        
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>  
        </javaTypeResolver>
        
        <javaModelGenerator targetPackage="com.mage.test.model" targetProject="${project.path}">  
			<property name="enableSubPackages" value="true"/>  
			<property name="trimStrings" value="true"/>  
        </javaModelGenerator>
        
        <sqlMapGenerator targetPackage="com.mage.test.mapping" targetProject="${project.path}">  
			<property name="enableSubPackages" value="true"/>  
        </sqlMapGenerator>
        
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.mage.test.dao" targetProject="${project.path}">  
        	<property name="enableSubPackages" value="true"/>  
        </javaClientGenerator>
        
        <table tableName="sy_user"
        	domainObjectName="User" enableCountByExample="fasle" 
        	enableUpdateByExample="fasle" enableDeleteByExample="fasle" 
        	enableSelectByExample="fasle" selectByExampleQueryId="fasle">
        	<!-- 忽略列，不生成bean 字段; 指定列的java数据类型  -->
        	<!--  
            <ignoreColumn column="FRED" />
            <columnOverride column="DOUBLE" jdbcType="DECIMAL" />
            -->
        </table>
        
    </context>  
</generatorConfiguration>